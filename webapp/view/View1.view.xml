<mvc:View controllerName="com.kumar.scan.BarcodeScanner.controller.View1" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:html="http://www.w3.org/1999/xhtml">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<HBox>
							<Button text="Start" id="startButton" press="onPressStart" class="sapUiTinyMarginEnd"></Button>
							<Button text="Reset" id="resetButton" press="onPressReset" class="sapUiTinyMarginEnd"></Button>
							<Select id="Select"></Select>
						</HBox>
						<VBox>
							
								
									<html:div>
										<html:video id="video" width="350" height="200" style="border: 1px solid gray" class="sapUiTinyMarginStart"></html:video>
									</html:div>
								
							
							<Label id="result" text="Result: "/>
						</VBox>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>